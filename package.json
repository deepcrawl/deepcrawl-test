{
  "name": "@deepcrawl/test-monorepo",
  "version": "0.0.1",
  "private": true,
  "description": "DeepCrawl Test monorepo.",
  "repository": "github:deepcrawl/deepcrawl-test",
  "workspaces": {
    "packages": [
      "packages/test-cli",
      "packages/test-nodejs-sdk"
    ]
  },
  "scripts": {
    "build": "wsrun --serial --exclude-missing build",
    "commit": "cz",
    "lint": "wsrun --serial lint:check",
    "postinstall": "yarn audit",
    "prettier:check": "prettier --list-different .",
    "prettier:fix": "prettier --write .",
    "release": "lerna publish",
    "prepare": "is-ci || husky install"
  },
  "resolutions": {
    "ws": "7.4.6"
  },
  "devDependencies": {
    "@types/jest": "27.0.1",
    "@types/node": "14.17.14",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "commitizen": "4.2.4",
    "eslint": "7.32.0",
    "eslint-config-deepcrawl": "5.9.0",
    "eslint-plugin-array-func": "3.1.7",
    "eslint-plugin-clean-code": "0.1.12",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-json-format": "2.0.1",
    "eslint-plugin-no-loops": "0.3.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-sonarjs": "0.10.0",
    "git-cz": "4.7.6",
    "husky": "7.0.2",
    "is-ci": "3.0.0",
    "jest-junit": "12.2.0",
    "lerna": "4.0.0",
    "postinstall-prepare": "2.0.0",
    "prettier": "2.3.2",
    "ts-node": "10.2.1",
    "typescript": "4.4.2",
    "wsrun": "5.2.4",
    "yarnhook": "0.5.1"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
